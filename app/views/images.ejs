<table ng-controller="ImagesList" class="table table-hover table-condensed">
  <thead>
    <tr>
      <th>Image Id</th>
      <th>Tags</th>
      <th>Created</th>
      <th>Size</th>
      <th>Virtual Size</th>
      <th>Parent</th>
    </tr>
  </thead>
  <tbody>
    <tr ng-repeat="img in images">
      <td><a href="/image/{{img.Id}}">{{img.Id.substring(0,TDSIZE)}}</a></td>
      <td>
        <div ng-repeat="tag in img.RepoTags">{{tag}}</div>
      </td>
      <td>{{img.Created * 1000 | date:'medium'}}</td>
      <td>{{img.Size | bytes}}</td>
      <td>{{img.VirtualSize | bytes}}</td>
      <td><a href="/image/{{img.ParentId}}">{{img.ParentId.substring(0,TDSIZE)}}</a></td>
    </tr>
  </tbody>
</table>
